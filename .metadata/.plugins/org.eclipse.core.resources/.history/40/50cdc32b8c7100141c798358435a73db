package lab2111;

import java.util.Scanner;

public class ParniNeParni {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
	 int uneseniBroj=in.nextInt();
	 int i=0;
	 int j=0;
	 int mjeracParni=0;
	 int mjeracNeparni=0;
	 String parni="";
	 String neparni="";
	 
	 for (i=0;i<uneseniBroj;i++){
	   if	 (i%2==0) {
	    	parni=parni+i+" " ;
	    	mjeracParni++;
	   }
	    
	  }
	 System.out.println(parni);
	 
	 for (j=0;j<uneseniBroj;j++){
		    if	 (j%2!=0) {
		    	System.out.println("neparni broj: "+ j);
	 mjeracNeparni++;
		 
	}

}

}}