
public class Damir {

	public static void main(String[] args) {
	if (args.length==0)
	{ System.out.println("Hello world" );
	}
     args[0].equals("r");
     args[1].equals("p"); 
     args[2].equals("t");
     args[3].equals("sha mai");
     
     if(args[1].equals("p"))
     for (int i=0;0< args.length-1; i++){
    	 System.out.println("argument p" + args[i]);
    	 
    	 
    	 
    	 if(args[1].equals("sha mai"))
    	     for (int j=0;0< args.length-1; j++){
    	    	 System.out.println("argument sha mai" + args[i]);
    	 
     }
	}

}
}