package lab5;

import java.util.Scanner;

public class skener {
public static void main(String[] args) {
	
	Scanner in = new Scanner(System.in); 
System.out.println("Unesite broj:");
int broj = in.nextInt();
System.out.println("Unesite drugi broj broj:");
double brojDva= in.nextInt();
System.out.print( "zbir:" );
System.out.println(broj +brojDva);
System.out.println("Oduzimanje: " + (broj - brojDva) );
System.out.println("mnozenje: " + (broj*brojDva));
System.out.println("djeljenje : "+ (broj/brojDva));
System.out.println("Korjen" + (Math.sqrt(broj+brojDva)));

 String uzorak = in.next();

}
}
