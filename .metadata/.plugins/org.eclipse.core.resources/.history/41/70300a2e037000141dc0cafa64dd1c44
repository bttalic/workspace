package lab1911;

public class Trougao {
public static void main(String[] args) {
	System.out.println("Izeberite prvu stranicu trougla");
double stranica1=TextIO.getDouble();
System.out.println("Izeberite drugu stranicu trougla");
double stranica2=TextIO.getDouble();
System.out.println("Izeberite trecu stranicu trougla");
double stranica3=TextIO.getDouble();

if ((stranica1<stranica2+stranica3) || (stranica2<stranica1+stranica3) || (stranica3<stranica1+stranica2)){
	System.out.println("Cestitamo vase stranice zadovoljavaju karakteristike trougla.");
}
}
}
