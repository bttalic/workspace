package lab5;

import java.util.Scanner;

public class Changecalculator {
public static void main(String[] args) {
	Scanner in = new Scanner (System.in);
	System.out.println("How many quarters");
	double number = in.nextInt();
	double quarter =(number * 0.25);
	
	System.out.println("How many dimes");
	int numberJedan = in.nextInt();
	double dime =(numberJedan * 0.10);
	
	System.out.println("How many nickles");
	int numberDva = in.nextInt();
	double nickle=(numberDva * 0.5);
	
	System.out.println("How many peenys");
	int numberTri = in.nextInt();
	double penny =(numberTri * 0.01);
	
	
	System.out.println(quarter+dime+nickle+penny);
}}
