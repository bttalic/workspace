package lab1911;

public class PadTlo {

	public static void main(String[] args) {
		System.out.println("Visina od tla");
		double visina= TextIO.getDouble();
		System.out.println("Vrijeme PADA");
		double vrijemePada = TextIO.getDouble();
	
		double ostatak;
		double gravitacija=9.81;
		vrijemePada= Math.sqrt((2*visina)/gravitacija);
		visina=(1*gravitacija*(vrijemePada*vrijemePada))/2;
		
	}
	
	
	
	}

