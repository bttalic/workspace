package najveci;

public class NajveciNiz {

	public static void main(String[] args) {
		int[] arr3 =new int[]{1, 2, 4};
		System.out.println(getMax(1, 2, 4));
	}
public static int getMax(int[] niz3) {
	int a=niz3[0];
	int b= niz3[1];
	int c= niz3[2];
	
	if (a>b && a> c) {
		return a;
		}
	else if (b>c) {
		return b;}
	return c;
	
}
}
