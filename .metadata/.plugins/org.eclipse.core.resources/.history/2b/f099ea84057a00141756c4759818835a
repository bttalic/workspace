
public class Damir {

	public static void main(String[] args) {
	
		
     args[1].equals("p"); 
     args[2].equals("t");
     args[3].equals("sha");
		
		
		if (args.length==0)
	{ System.out.println("Hello world" );
	}
     
     
     if(args[1].equals("p"))
     for (int i=0;0< args.length-1; i++){
    	 System.out.println("argument p" + args[1]);
    	 
     }
    	 
    	    	 
    	 if(args[1].equals("sha"))
    	     for (int j=0;0< args.length-1; j++){
    	    	 System.out.println("argument sha mai" + args[3]);
    	     }
    	 
     }
	

}
