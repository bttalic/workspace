
	public class textsabiranje {
	public static void main(String[] args) {

	 TextIO.writeUserSelectedFile();
   System.out.println("Unesite vase ime: ");
   String ime = TextIO.getln();
   System.out.println("Unesite vase godine: ");  
   int godine = TextIO.getInt();
   System.out.println("Unesite  vasu lokaciju: "); 
   String lokacije = TextIO.getln();
   System.out.println("Unesite neki broj"); 
   int nekiBroj = TextIO.getInt();
   
   System.out.println("Hvala spasili smo navedene informacije");

  
  System.out.printf(ime, godine, lokacije, nekiBroj);



}
}
	

