package lab2011;

public class Piramide {

	public static void pirmaida1 (int n){
	 for (int i = 1; i<=n; i++) { 
		 for ( int j=1;j<=i; j++)  {
			 System.out.printf("%-2d", j);
		 }
		 }System.out.println();
	 }
	
	/*
	*
	*
	*
	*Ipisuje piramidu
	*1
	*12
	*123
	*1234n
	*
	*
	*
	*/
	
	public static void main(String[] args) {
		pirmaida1(6);
	}
}
