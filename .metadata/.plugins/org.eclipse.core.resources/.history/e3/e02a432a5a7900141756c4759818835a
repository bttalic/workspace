import java.util.Scanner;

public class Cezarova {

	public static void main(String[] args) {
		
		String abeceda = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
		abeceda.toLowerCase();
		abeceda.toCharArray();
		Scanner in = new Scanner (System.in);
		String recenica="bitcamp";
		String finalni="";
		char[] niz=recenica.toCharArray();
		for(int i=0 ;i<recenica.length()-1 ;i++){
			for (int j=0 ;j<abeceda.length()-1 ;j++){
			niz[i]=abeceda.charAt(j-3);
		    if (niz[i]==abeceda.charAt(0)){
		    	niz[i]=abeceda.charAt(23);
		    }
		    if (niz[i]==abeceda.charAt(1)){
		    	niz[i]=abeceda.charAt(24);
		    }
		    if (niz[i]==abeceda.charAt(2)){
		    	niz[i]=abeceda.charAt(25);
		    }
			
			
			}
		}
	}
}
