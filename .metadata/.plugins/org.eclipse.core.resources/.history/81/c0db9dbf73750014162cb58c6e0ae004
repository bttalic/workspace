package lab26112014;

public class Prvi {

	public static int[] unosNiz() {
		int i = 0;
		System.out.println("Unesite Total brojeva");
		int[] niz = new int[TextIO.getlnInt()];
		for (i = 0; i < 5; i++) {
			System.out.println("Unesi broj");
			niz[i] = TextIO.getlnInt();

		}
		return niz;
	}

	public static void main(String[] args) {

		int[] niz = unosNiz();
		int i = 0;
		
		for (i = 0; i < niz.length; i++) {
			System.out.print(niz[i] + " ");

		}

	}

}
