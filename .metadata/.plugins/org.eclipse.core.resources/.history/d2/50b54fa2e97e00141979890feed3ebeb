package ArrayUtilTest;

import test.ArrayUtil;

public class TestnaKlasa {
public static void main(String[] args) {
	int broj=10;

	int[] popunjenUserNiz=ArrayUtil.getUserArray(broj);
	
  int[] popunjenNiz=ArrayUtil.getArray(broj, broj);
  
  ArrayUtil.copyArray(popunjenUserNiz, popunjenNiz);
  ArrayUtil.printArray(popunjenNiz);
}
}
