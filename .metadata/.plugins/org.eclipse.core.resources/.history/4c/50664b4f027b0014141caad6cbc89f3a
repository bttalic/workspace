import java.util.Scanner;


public class ZadatakMAtrica {
	
	/**Unos niza kojem morate unjeti broj mjesta i unjeti vrijednosti u int formatu
	 * 
	 * @ ulaz u metodu je niz
	 * @return ispisuje se niz sa unesenim podatcima korisnika
	 */
	public static int[][] unosNiza (int niz, int sirina){
		int varijabla=0;
		Scanner in= new Scanner(System.in);
		int i = 0;
	    int j = 0;
	    int[][] spremanNiz = new int[niz][sirina];
	    int[][] gotovNiz = new int[i][j];
		for(i = 0; i<spremanNiz.length;i++){
				for(j = 0; i<spremanNiz[j].length;j++){
							System.out.println("Unesite vrijednost: ");
							gotovNiz[i][j]+= in.nextInt();
	            }  
	  	}
	            System.out.println(spremanNiz[0][0]);
	    return   spremanNiz ;    
	}
	
