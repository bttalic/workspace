	public static int  trazi (int[] potragaNiz, int broj){
			for(int i = 0; i<potragaNiz.length;i++){
			 if (potragaNiz[i] == broj){
				return i;
				}
		}
					
		
		return -1;
	}
	
public static void main(String[] args) {
	
		Scanner in= new Scanner(System.in);
		System.out.println("Unesite duzinu niza");
		int duzinaNiza=in.nextInt();
		System.out.println("Unesite sirinu niza");
		int sirinaNiza=in.nextInt();
		int[][] ispisNiz = unosNiza(duzinaNiza,sirinaNiza);
		
		in.close();
}
}
