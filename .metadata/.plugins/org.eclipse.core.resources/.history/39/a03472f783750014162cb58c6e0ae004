package lab26112014;

public class Najmanji {
	
	public static int najManji(int[] burek){
		
		int[] niz = new int[burek];
		int mali=0;
		int i=0;
		for(i=0;i<niz.length;i++)
		if (mali > niz[i]){
			mali=niz[i];
		}
				
		return mali;} 
		
			
	
public static void main(String[] args) {
	
	 int i=0;
		System.out.println("Unesi broj");
		int totalBrojeva=TextIO.getlnInt();
		do {
		int[] niz =new int [TextIO.getlnInt()];
		i++;
		} while (totalBrojeva!=i);
		
		najManji(niz);
		System.out.println(mali);
}
}
