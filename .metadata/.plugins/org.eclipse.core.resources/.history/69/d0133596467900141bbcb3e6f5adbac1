
public class Kalenadar {
public static void main(String[] args) {
	System.out.println("======>" + godinaJePrestupna(1900));
	System.out.println("======>" + godinaJePrestupna(2000));
	System.out.println("======>" + godinaJePrestupna(2003));
	System.out.println("======>" + godinaJePrestupna(2004));
	System.out.println("======>" + godinaJePrestupna(2005));
	System.out.println("======>" + godinaJePrestupna(2006));
	System.out.println("======>" + godinaJePrestupna(2100));
	
}
	public static boolean godinaJePrestupna(int godina){
	if(godina%400==0){
		
		return true;
	}
	if (godina % 100==0){
		return false;
	}	
	if (godina%4==0){
	return true;
	}
	return false;
	}
}

