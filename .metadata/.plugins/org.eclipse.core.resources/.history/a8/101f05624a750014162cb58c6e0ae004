package najveci;

public class NajveciNiz {

	public static void main(String[] args) {
		int[] arr3 =new int[]{1, 2, 4};
		System.out.println(arr3);
	}
public static int getMax(int[] brojevi) {
	int max= brojevi[0];
	for (int idx=1; idx<brojevi.length; idx++)
	{int trenutni = brojevi [idx];
		if (trenutni> max) {
        max =trenutni;
		
	}
}
	return max;
	
}
}
